<div class="publication">
	<%= form_for(@publication)  do |f| %>
		<fieldset class="publication" >
			<%= render 'shared/error_messages', object: f.object %>
			<ul class="publication">
				<li>
					<%= f.label :title %>
					<%= f.text_field :title %>
				</li>
				<li>	
					<%= f.label :published %>
					<%= f.text_field :published %>
				</li>			
				<li>
					<%= f.label :description %>
					<%= f.text_field :description %>
				</li>		
				<li>
					<%= f.submit "Update publication", class: "btn btn-primary" %>
				</li>		
			</ul>	
		</fieldset>
	<% end %>
</div>




